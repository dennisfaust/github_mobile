<div id="pageTitle">
	<h1><%= @user.id%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @user.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @user.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">User</td>
        <td class="itemValue"><%= @user.user%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Login</td>
        <td class="itemValue"><%= @user.login%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Email</td>
        <td class="itemValue"><%= @user.email%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Location</td>
        <td class="itemValue"><%= @user.location%></td>
      </tr>
      
    </table>
</div>